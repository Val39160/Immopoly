<div class="edit-container">
  <div>
    <h1 class="edit-h1">Edit Property</h1>
  </div>
  <div class="edit-form-container">
    <div class="edit-h3">
    </div>
  <div class="edit-form">
      <%= form_with model: @property do |f| %>
      <div class="edit-property-address">
        <h2></h2>
      </div>
      <div class="edit-input">
        <%= f.label :Surface_terrain %>
      </div>
      <div class="edit-input">
        <%= f.number_field :nb_dwelling %>
      </div>
      <div class="edit-input">
        <%= f.label :area_sqm_land %>
      </div>
      <div class="edit-input">
        <%= f.number_field :area_sqm_land %>
      </div>
      <div class="edit-input">
        <%= f.label :Surface_habitable %>
      </div>
      <div class="edit-input">
        <%= f.number_field :area_sqm_livable %>
      </div>
      <div class="edit-input">
        <%= f.label :Ratio_surface_deplancher %>
      </div>
      <div class="edit-input">
        <%= f.number_field :floor_area_ratio %>
      </div>
      <div class="edit-input">
        <%= f.label :building_height_floors %>
      </div>
      <div class="edit-input">
        <%= f.number_field :building_height_floorsg %>
      </div>
      <div class="edit-input">
        <%= f.label :nb_rooms_per_dwelling %>
      </div>
      <div class="edit-input">
        <%= f.number_field :nb_rooms_per_dwelling %>
      </div>
      <div class="edit-input">
        <%= f.label :energy_class %>
      </div>
      <div class="edit-input">
        <%= f.number_field :energy_class %>
      </div>
      <div>
        <%= f.submit "Soumettre", class: "edit-input-button" %>
      </div>
      <% end %>
    </div>
  </div>
</div>
